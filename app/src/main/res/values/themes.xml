<resources>
    <!-- Base application theme. -->
    <style name="Theme.Census" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#6495ED</item>
        <item name="colorPrimaryVariant">#00008B</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Button_style">
        <item name="android:backgroundTint">#55697a</item>
        <item name="android:textColor">#77D4D3</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    </resources>